{
  "name": "pipestack-wc",
  "version": "0.1.0",
  "description": "wc - word, line, and byte or character count",
  "main": "wc.js",
  "scripts": {
    "browserify": "browserify --standalone pipestack.wc lib/wc.js > pipestack-wc.js",
    "browserify-min": "browserify --standalone pipestack.wc -p tinyify lib/wc.js > pipestack-wc.min.js",
    "lint": "eslint lib/*",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "compare-test": "mocha --reporter spec --bail --check-leaks compare-test/",
    "compare-test-docker": "node ./compare-test/compare-tests-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pipestack-io/pipestack-wc.git"
  },
  "keywords": [
    "wc"
  ],
  "author": "Pipestack.io",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/pipestack-io/pipestack-wc/issues"
  },
  "homepage": "https://github.com/pipestack-io/pipestack-wc#readme",
  "devDependencies": {
    "browserify": "^16.5.0",
    "eslint": "^6.5.1",
    "mocha": "6.2.1",
    "should": "13.2.3",
    "tinyify": "^2.5.1"
  }
}
